# Local development environment variables
# This file is copied to .env.local by start-all.sh
# DO NOT commit .env.local to the repository

# Demo mode configuration
# Set to false for local development with full backend features
# Set to true for Vercel demo deployment (read-only mode)
NEXT_PUBLIC_DEMO_MODE=false

# Vercel environment detection
# Set to 0 for local development to prevent Vercel-specific errors
VERCEL=0

# Environment type
# Used by Next.js for environment-specific behavior
NEXT_PUBLIC_VERCEL_ENV=development
